<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="MauiOrientationTest.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:MauiOrientationTest.Views"
	xmlns:local="clr-namespace:MauiOrientationTest">

	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Main"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Blank"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:BlankPage}"
		Route="BlankPage" />
</Shell>
